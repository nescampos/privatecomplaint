{
  "name": "sunset",
  "version": "1.0.0",
  "type": "module",
  "description": "A Midnight Network application created with create-mn-app",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run proof-server\" \"npm run watch\"",
    "watch": "nodemon --config nodemon.json",
    "setup": "npm run compile && npm run build && npm run deploy",
    "compile": "compact compile contracts/compliants.compact contracts/managed/compliants",
    "build": "tsc",
    "deploy": "node dist/deploy.js",
    "cli": "node dist/cli.js",
    "check-balance": "npm run build && node dist/check-balance.js",
    "health-check": "npm run build && node dist/health-check.js",
    "web": "npm run build && node dist/web-server.js",
    "reset": "rm -rf contracts/managed deployment.json dist && npm run compile",
    "validate": "tsc --noEmit && npm run compile",
    "clean": "rm -rf dist contracts/managed deployment.json",
    "proof-server": "docker run -p 6300:6300 midnightnetwork/proof-server -- 'midnight-proof-server --network testnet'",
    "dev:web": "cd web && npx next dev",
    "build:web": "cd web && npx next build",
    "start:web": "cd web && npx next start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "^0.9.0",
    "@midnight-ntwrk/dapp-connector-api": "^3.0.0",
    "@midnight-ntwrk/ledger": "^4.0.0",
    "@midnight-ntwrk/midnight-js-contracts": "2.0.2",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-network-id": "2.0.2",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-types": "2.0.2",
    "@midnight-ntwrk/wallet": "5.0.0",
    "@midnight-ntwrk/wallet-api": "5.0.0",
    "@midnight-ntwrk/zswap": "^4.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "chalk": "^5.3.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "lucide-react": "^0.553.0",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "midnight",
    "blockchain",
    "zero-knowledge",
    "privacy"
  ]
}
